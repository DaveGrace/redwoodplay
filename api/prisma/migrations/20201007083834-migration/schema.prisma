datasource DS {
  // optionally set multiple providers
  // example: provider = ["sqlite", "postgresql"]
  provider = "sqlite"
  url = "***"
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model MembersDB {
  id    Int    @id @default(autoincrement())
  email String @unique

}

model Org {
  id             Int    @id @default(autoincrement())
  email          String @unique
  phone          Int?
  name           String
  street_address String
  suburb         String
  state          String
  postcode       Int
}
